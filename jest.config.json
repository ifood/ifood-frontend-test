{
  "globals": {},
  "testURL": "http://127.0.0.1:8080",
  "coverageThreshold": {
    "global": {
      "branches": 50,
      "functions": 50,
      "lines": 50,
      "statements": 50
    }
  },
  "coveragePathIgnorePatterns": [
    "<rootDir>/app/tests/"
  ],
  "coverageDirectory": "<rootDir>/coverage",
  "moduleNameMapper": {
    "\\.(html|jpg|jpeg|png|gif|svg|ttf|woff|woff2)$": "<rootDir>/app/__mocks__/assetsMock.js",
    "\\.(scss|css|less)$": "<rootDir>/app/__mocks__/assetsMock.js",
    "^Helpers(.*)$": "<rootDir>/app/helpers/$1",
    "^Providers(.*)$": "<rootDir>/app/providers/$1"
  },
  "snapshotSerializers": [
    "enzyme-to-json/serializer"
  ],
  "setupFiles": [
    "<rootDir>/app/tests/support/requestAnimationFrame.js",
    "<rootDir>/app/tests/support/setupTests.js"
  ],
  "testPathIgnorePatterns": [],
  "modulePaths": [
    "<rootDir>/app/"
  ]
}
